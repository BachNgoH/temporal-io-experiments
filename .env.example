# Temporal Configuration
TEMPORAL_HOST=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=default-task-queue
TEMPORAL_USE_CLOUD=false

# Optional: Temporal Cloud (if using managed Temporal)
# TEMPORAL_CERT_PATH=/path/to/cert.pem
# TEMPORAL_KEY_PATH=/path/to/key.pem

# Worker Configuration
WORKER_MODE=base

# Application
APP_NAME=Temporal Task System
APP_VERSION=0.1.0
DEBUG=false

# GCP Configuration (for deployment)
GCP_PROJECT_ID=your-project-id
GCP_REGION=us-central1
GCP_INSTANCE_NAME=ai-core-instance
GCP_ZONE=us-central1-a

# Compute Engine External IP (will be auto-detected)
COMPUTE_ENGINE_EXTERNAL_IP=
